config usbleach 'main'
	option enabled 'off'
	option yarascan 'off'
	option allow_dangerous 'on'
	option docbleach_enabled 'on'
	option only_one_user 'on'
	option email_source 'usbleach@localdomain'
	option use_whitelist 'on'
	list ext_whitelist 'pdf'
	list ext_whitelist 'doc'
	list ext_whitelist 'docx'
	list ext_whitelist 'docm'
	list ext_whitelist 'xls'
	list ext_whitelist 'xlsx'
	list ext_whitelist 'xlst'
	list ext_whitelist 'txt'
	list ext_whitelist 'sh'
	list ext_whitelist 'js'
	list ext_whitelist 'html'
	list ext_whitelist 'css'
	list ext_whitelist 'png'
	list ext_whitelist 'exif'
	list ext_whitelist 'tiff'
	list ext_whitelist 'tif'
	list ext_whitelist 'jpg'
	list ext_whitelist 'jpeg'
	list ext_whitelist 'ppt'
	list ext_whitelist 'pptx'
	list ext_whitelist 'gif'
	list ext_whitelist 'ico'
	list ext_whitelist 'mp3'
	list ext_whitelist 'mp4'
	list ext_whitelist 'h'
	list ext_whitelist 'c'
	list ext_whitelist 'php'
	list ext_whitelist 'java'
